{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "ES2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ES2015"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@pages/*": ["./modules/dashboard/pages/*"],
      "@components/*": ["./modules/dashboard/components/*"],
      "@hoc/*": ["./modules/common/hoc/*"],
      "@commonCom/*": ["./modules/common/commonComponent/*"],
      "@input/*": ["./modules/common/input/*"]
    }
  },
  "include": [
    "src"
  ],
  "experimentalDecorators": true,
  "noImplicitAny": false,
  "strictNullChecks": false
}
