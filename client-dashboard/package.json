{
  "name": "bioandme-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.staging react-app-rewired start",
    "build:development": "env-cmd -f .env.development react-app-rewired build",
    "build:staging": "env-cmd -f .env.staging react-app-rewired build",
    "build:stg": "env-cmd -f .env.stg react-app-rewired build",
    "build:stg2": "env-cmd -f .env.stg2 react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "build:production": "env-cmd -f .env.production react-app-rewired build",
    "deploy:staging": "aws s3 sync build/ s3://user.v2.dev.bioandme.asia --cache-control no-cache",
    "deploy:stg": "aws s3 sync build/ s3://eg-stg-amili-v2-live-chat-origin --cache-control no-cache",
    "deploy:stg2": "aws s3 sync build/ s3://eg-stg2-amili-v2-live-chat-origin --cache-control no-cache",
    "deploy:prod": "aws s3 sync build/ s3://eg-prod-amili-v2-live-chat-origin --cache-control no-cache",
    "invalidate:staging": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E3HJRIOZ0CAL9L --paths '/*'",
    "invalidate:stg": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E1C9JM3VTJAK5H --paths '/*'",
    "invalidate:stg2": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E2KBPLFSRO6Y94 --paths '/*'",
    "invalidate:prod": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E1YUZ4N0QU7QQH --paths '/*'",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "preloadLink": "node ./addPreloadLink.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.5",
    "@stripe/stripe-js": "1.35.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react-custom-scrollbars": "4.0.10",
    "@types/react-router": "5.1.18",
    "antd": "4.23.0",
    "aws-sdk": "2.1209.0",
    "axios": "0.27.2",
    "chalk": "5.0.1",
    "chart.js": "3.9.1",
    "chartjs-plugin-annotation": "2.0.1",
    "chartjs-plugin-datalabels": "2.1.0",
    "cross-env": "7.0.3",
    "dayjs": "1.11.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fontfaceobserver": "2.3.0",
    "formik": "2.2.9",
    "html5-qrcode": "2.2.1",
    "husky": "8.0.1",
    "i18next": "21.9.1",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-scanner": "4.0.0",
    "immutable": "4.1.0",
    "jest-styled-components": "7.1.1",
    "jwt-decode": "3.1.2",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.37",
    "node-plop": "0.31.0",
    "plop": "3.1.1",
    "prettier": "2.7.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-beautiful-dnd": "13.1.1",
    "react-chartjs-2": "4.3.1",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "18.2.0",
    "react-ga4": "1.4.1",
    "react-helmet-async": "1.3.0",
    "react-highlight-words": "0.18.0",
    "react-i18next": "11.18.5",
    "react-infinite-scroller": "1.2.6",
    "react-input-mask": "2.0.4",
    "react-joyride": "2.5.2",
    "react-query": "3.39.2",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-router-redux": "4.0.8",
    "react-scripts": "5.0.1",
    "react-slick": "0.29.0",
    "react-test-renderer": "18.2.0",
    "react-verification-code-input": "1.2.9",
    "react-youtube": "9.0.3",
    "redux-injectors": "2.0.0",
    "redux-persist": "6.0.0",
    "redux-saga": "1.2.1",
    "reselect": "4.1.6",
    "rimraf": "3.0.2",
    "sanitize.css": "13.0.0",
    "sass": "1.54.8",
    "serve": "14.0.1",
    "shelljs": "0.8.5",
    "slick-carousel": "1.8.1",
    "styled-components": "5.3.5",
    "stylelint": "14.11.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "web-vitals": "3.0.1",
    "xlsx": "0.18.5",
    "yarn": "1.22.19",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@stripe/react-stripe-js": "1.10.0",
    "@types/fontfaceobserver": "2.1.0",
    "@types/jest": "29.0.0",
    "@types/lodash": "4.14.184",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-infinite-scroller": "1.2.3",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux-persist": "4.3.1",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.18.0",
    "babel-plugin-import": "1.13.5",
    "customize-cra": "1.0.0",
    "env-cmd": "10.1.0",
    "less": "4.1.3",
    "less-loader": "11.0.0",
    "react-app-rewired": "2.2.1"
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
