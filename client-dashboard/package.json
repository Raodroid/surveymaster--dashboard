{
  "name": "bioandme-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.dev vite",
    "build:development": "env-cmd -f .env.development tsc && vite build",
    "build:dev-server": "env-cmd -f .env.dev-server tsc && vite build",
    "build:stg": "env-cmd -f .env.stg tsc && vite build",
    "deploy:dev-server": "aws s3 sync build/ s3://eg-dev-server-survey-master-admin-origin --cache-control no-cache",
    "deploy:stg": "aws s3 sync build/ s3://sm-stg-survey-master-admin-origin --cache-control no-cache",
    "invalidate:dev-server": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E1FZTWZAHX0593 --paths '/*'",
    "invalidate:stg": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id EI45866BA68ES --paths '/*'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage .",
    "test:debug": "jest --inspect-brk --runInBand --no-cache",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "preloadLink": "node ./addPreloadLink.js"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@reduxjs/toolkit": "1.8.5",
    "@stripe/stripe-js": "1.35.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react-custom-scrollbars": "4.0.10",
    "@types/react-router": "5.1.18",
    "@vitejs/plugin-react": "4.1.1",
    "antd": "4.23.0",
    "array-move": "4.0.0",
    "aws-sdk": "2.1209.0",
    "axios": "0.27.2",
    "dayjs": "1.11.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "formik": "2.2.9",
    "i18next": "21.9.1",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-scanner": "4.0.0",
    "immutable": "4.1.0",
    "jest-styled-components": "7.1.1",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "prettier": "2.7.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-beautiful-dnd": "13.1.1",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.5",
    "react-infinite-scroller": "1.2.6",
    "react-input-mask": "2.0.4",
    "react-query": "3.39.2",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-router-redux": "4.0.8",
    "react-sortable-hoc": "2.0.0",
    "react-verification-code-input": "1.2.9",
    "redux-injectors": "2.0.0",
    "redux-persist": "6.0.0",
    "redux-saga": "1.2.1",
    "reselect": "4.1.6",
    "rimraf": "3.0.2",
    "sanitize.css": "13.0.0",
    "sass": "1.54.8",
    "simplebar-react": "2.4.3",
    "styled-components": "5.3.5",
    "vite": "4.5.0",
    "vite-plugin-babel-macros": "1.0.6",
    "vite-plugin-svgr": "4.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "web-vitals": "3.0.1",
    "xlsx": "0.18.5",
    "yup": "0.32.11"
  },
  "overrides": {
    "babel-plugin-macros": "^3.0.1"
  },
  "devDependencies": {
    "@stripe/react-stripe-js": "1.10.0",
    "@testing-library/dom": "9.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.7",
    "@types/lodash": "4.14.184",
    "@types/node": "20.8.10",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-infinite-scroller": "1.2.3",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux-persist": "4.3.1",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.18.0",
    "autoprefixer": "10.4.16",
    "babel-plugin-import": "1.13.5",
    "customize-cra": "1.0.0",
    "env-cmd": "10.1.0",
    "husky": "8.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "less": "4.1.3",
    "less-loader": "7.3.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.31",
    "react-error-overlay": "6.0.9",
    "react-test-renderer": "18.2.0",
    "serve": "14.0.1",
    "stylelint": "14.11.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "tailwindcss": "3.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "yarn": "1.22.19"
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=16.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jest-environment-jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}
